{
	"authors": [{
		"name": "Kyle Niemiec",
		"email": "kyle@codeflower.io"
	}],
	"autoload": {
		"psr-4": {
			"WPPF\\": "",
			"WPPF\\Tests\\": "tests/"
		}
	},
	"bin": ["bin/wppf", "bin/xphp"],
	"description": "My framework for facilitating WordPress plugin development.",
	"license": "GPL-3.0",
	"name": "kyle-niemiec/wp-plugin-framework",
	"require-dev": {
		"symfony/console": "^6.4",
		"code-lts/doctum": "^5.5",
		"phpunit/phpunit": "^10.5"
	},
	"scripts": {
		"cache:clear": "rm -rf ./.doctum-cache ; mkdir ./.doctum-cache",
		"docs:clean": "rm -rf ./docs",
		"docs:mkdocs": "mkdocs build --clean",
		"docs:api": "mkdir -p ./docs/api && php -d error_reporting=\"E_ALL & ~E_DEPRECATED & ~E_USER_DEPRECATED\" ./vendor/bin/doctum.php update ./doctum.php",
		"docs:build": "composer docs:clean ; composer docs:mkdocs ; composer docs:api",
		"framework:version:upgrade": "./bin/wppf framework:version:upgrade",
		"tests:run:all": "bin/xphp vendor/bin/phpunit -c ./phpunit.xml"
	},
	"type": "library",
	"version": "1.2.2"
	
}
